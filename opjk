
mylist = [40, 20, 43, 56, 98, 89, 90, 87]
mylist[1:2] = [58, 300]
mylist[1:5] = [2000, 300]
mylist[0:4] = [200, 300]
mylist[0:6] = [200, 3000]
mylist[1:3] = [200, 300]
mylist[-6:2] = [200, 300]
mylist[1:-8] = [200, 300]
mylist[1:-6] = [200, 300]
mylist[-1:6] = [200, 300]
print(mylist)
mylist.clear()
print(mylist)
# arithmetic operator
print(100+10)
print(1056-20)
print(1090-20)
print(107*20)
print(106/20)
print(105//20)
print(105 % 20)
print(109**20)
